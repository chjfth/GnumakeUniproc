
.PHONY: _GSLO_DoStrip
_GSLO_DoStrip: 
	@if [ ! $(gmu_Gp_LINK_OUTPUT) -ot $${gmu_p_BuildStartCkt} ]; then \
		echo "$(_pluginId_GCC_STRIP_LO): Stripping link-output: $(notdir $(gmu_Gp_LINK_OUTPUT)) ..."; \
		$(gmp_GCC_PREFIX)strip $(gmu_Gp_LINK_OUTPUT); \
	fi;
# We use -ot instead of -nt, because, in theory, we should do the strip 
#if the time of $(gmu_Gp_LINK_OUTPUT) and $(gmu_p_BuildStartCkt) are equal.
